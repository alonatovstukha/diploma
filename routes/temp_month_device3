function getRandomInt(max) {
  return Math.floor(Math.random() * Math.floor(max));
}

const dataMonth1 = new DataDevice3({data: getRandomInt(10).toString(),  date: '1', day: '0', hours: '13', minutes: '0', seconds: '20'});
